{
  "name": "ngbp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "check-dependencies --verbose && ng serve",
    "hmr": "check-dependencies --verbose && ng serve --configuration hmr",
    "build": "ng build",
    "test": "ng test",
    "check-dependencies": "check-dependencies --scope-list dependencies --scope-list devDependencies --install",
    "test-headless": "npm run check-dependencies && ng test --browsers ChromeHeadless",
    "sass-lint": "sass-lint -c .sass-lint.yml -v -q",
    "lint": "ng lint && npm run sass-lint",
    "e2e": "ng e2e --port=4201"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run check-dependencies && npm run lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.2",
    "@angular/cdk": "^9.0.1",
    "@angular/common": "~9.0.2",
    "@angular/compiler": "~9.0.2",
    "@angular/core": "~9.0.2",
    "@angular/forms": "~9.0.2",
    "@angular/localize": "^9.0.2",
    "@angular/material": "^9.0.1",
    "@angular/material-moment-adapter": "^9.0.1",
    "@angular/platform-browser": "~9.0.2",
    "@angular/platform-browser-dynamic": "~9.0.2",
    "@angular/router": "~9.0.2",
    "core-js": "^3.6.4",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "npm": "^6.13.7",
    "pa11y": "^5.3.0",
    "pa11y-reporter-html": "^1.0.0",
    "rxjs": "~6.5.4",
    "sass-lint": "^1.13.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "^9.0.3",
    "@angular/compiler-cli": "^9.0.2",
    "@angular/language-service": "~9.0.2",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "check-dependencies": "^1.1.0",
    "codelyzer": "^5.1.2",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-bullet": "^1.0.3",
    "protractor": "^5.4.3",
    "ts-node": "~7.0.0",
    "tslint": "^5.20.1",
    "typescript": "3.7.5"
  }
}
